        <id>Portable.MvvmLightLibs</id>
        <version>4.2-beta1</version>
        <title>MVVM Light libraries only (PCL)</title>
        <authors>Laurent Bugnion (GalaSoft),onovotny</authors>
        <owners>onovotny</owners>
        <licenseUrl>http://mvvmlight.codeplex.com/license</licenseUrl>
